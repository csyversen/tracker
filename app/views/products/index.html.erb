<div class="product list">
  <h1>All products</h1>
  <div><%= pluralize(@products.size, "product") %> found</div>
  <table class="table table-striped table-hover" summary="product list">
    <tr class="header">
      <th></th>
      <th>Product</th>
      <th>Current Price</th>
      <th>Lowest Price</th>
    </tr>
    <%= will_paginate %>
      <%= render @products %>
    <%= will_paginate %>
  </table>
</div>
